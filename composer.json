{
    "name": "cpeople-team/portable-ascii",
    "description": "Fork of Portable ASCII library - performance optimized (ascii) string functions for PHP by CreativePeople.",
    "type": "library",
    "keywords": [
        "clean",
        "php",
        "ascii"
    ],
    "homepage": "https://github.com/cpeople-team/portable-ascii",
    "license": "MIT",
    "authors": [
        {
            "raising": "CreateivePeople",
            "homepage": "https://cpeople.team",
            "email": "info@cpeople.team"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0 || ~7.0 || ~9.0"
    },
    "suggest": {
        "ext-intl": "Use Intl for transliterator_transliterate() support"
    },
    "autoload": {
        "psr-4": {
            "voku\\": "src/voku/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "voku\\tests\\": "tests/"
        }
    }
}
